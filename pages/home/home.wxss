.home {
  width: 83%;
  margin: 0 auto;
}
.title {
  font-size: 48rpx;
  width: 227rpx;
  height: 67rpx;
  line-height: 67rpx;
}
.nav {
  width: 100%;
  height: 150rpx;
  box-shadow:0rpx 0rpx 6rpx 0rpx rgba(0,0,0,0.17);
  border-radius:10rpx;
  margin-top: 40rpx;
  margin-bottom: 50rpx;
  display: flex;
}
.navImg {
  width: 33rpx;
  height: 38rpx;
  margin: 47rpx 40rpx;
}
.navText {
  width: 90%;
  margin-top: 20rpx;
}
.navText_1 {
  font-size: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
}
.navText_2 {
  font-size: 22rpx;
  height: 25rpx;
  line-height: 25rpx;
  color: #9B9B9B;
}
.myfunc {
  font-size: 33rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-bottom: 1px solid #E0E0E0;
  padding-bottom: 18rpx;
}
.home_1 {
  height:53rpx;
  font-size:32rpx;
  color:#000000;
  line-height:45rpx;
  font-family:PingFangSC-Regular;
}
.owner {
  width:234rpx;
  height:161rpx;
  background:linear-gradient(180deg,rgba(34,212,147,1) 0%,rgba(0,191,190,1) 100%);
  border-radius:10rpx;
  margin: 27rpx;
  margin-left: 0rpx;
  position: relative;
}
.ownerImg {
  width:56rpx;
  height:56rpx;
  position: absolute;
  top: 27rpx;
  left: 40%;
}
.ownerTitle, .carTitle {
  color: #ffffff;
  font-size:32rpx;
  float: left;
  margin-top: 80rpx;
  margin-left: 33rpx;
}
.car {
  width:357rpx;
  height:345rpx;
  background:linear-gradient(135deg,rgba(63,191,255,1) 0%,rgba(0,201,210,1) 100%);
  border-radius:10rpx;
  float: right;
  margin-top: -370rpx;
  position: relative;
}
.carImg {
  width:54rpx;
  height:44rpx;
  padding: 119rpx 130rpx 0rpx;
  padding-bottom: 17rpx;
}
.carTitleT {
  height:45rpx;
  font-size:32rpx;
  line-height: 45rpx;
  color: #ffffff;
  position: absolute;
  top: 180rpx;
  left: 135rpx;
}